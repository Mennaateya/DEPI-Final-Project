<svg xmlns="http://www.w3.org/2000/svg" width="300" height="300" viewBox="0 0 300 300">
  <!-- Background representing an X-ray or medical scan -->
  <rect width="300" height="300" fill="#e0e0e0"/>
  
  <!-- Medical image simulation -->
  <ellipse cx="150" cy="150" rx="100" ry="120" fill="#a0a0a0" stroke="#808080" stroke-width="2"/>
  
  <!-- Simulated anatomical features -->
  <circle cx="150" cy="130" r="30" fill="#d0d0d0" stroke="#808080" stroke-width="1"/>
  <path d="M100,180 Q150,230 200,180" fill="none" stroke="#808080" stroke-width="2"/>
  
  <!-- Simulated abnormality/region of interest -->
  <circle cx="180" cy="120" r="15" fill="#b0b0b0" stroke="#606060" stroke-width="2"/>
  
  <!-- Measurement markers -->
  <line x1="160" y1="90" x2="200" y2="90" stroke="#ffffff" stroke-width="1"/>
  <line x1="160" y1="90" x2="160" y2="95" stroke="#ffffff" stroke-width="1"/>
  <line x1="200" y1="90" x2="200" y2="95" stroke="#ffffff" stroke-width="1"/>
  
  <!-- Text to simulate medical imaging labels -->
  <text x="10" y="20" font-family="Arial" font-size="10" fill="#ffffff">MEDICAL SCAN</text>
  <text x="10" y="290" font-family="Arial" font-size="8" fill="#ffffff">TEST IMAGE</text>
</svg>